onents/ComponentA.vue'\nimport ComponentB from './components/ComponentB.vue'\nexport default {\n  components: {\n    ComponentA,\n    ComponentB\n  },\n  data() {\n    return {\n      show: true,\n      myAnimation: '',\n      myComponent: \"ComponentA\"\n    }\n  },\n  methods: {\n    beforeEnter() {\n      // el.style.transform = 'scale(0)'\n    },\n    enter(el, done) {\n      let scale = 0;\n      const interval = setInterval(() => {\n        el.style.transform = `sacale(${scale})`;\n        scale += 0.1;\n        if (scale > 1) {\n          clearInterval(interval);\n          done();\n        }\n      }, 200)\n    },\n    afterEnter() {\n\n    },\n    enterCancelled() {\n\n    },\n    beforeLeave() {\n\n    },\n    leave(el, done) {\n      let scale = 1;\n      const interval = setInterval(() => {\n        el.style.transform = `sacale(${scale})`;\n        scale -= 0.1;\n        if (scale < 0) {\n          clearInterval(interval);\n          done();\n        }\n      }, 20)\n    },\n    afterLeave() {\n\n    },\n    leaveCancelled() {\n\n    }\n  }\n\n}\n</script>\n\n<style scoped>\n.circle {\n  width: 200px;\n  height: 200px;\n  margin: auto;\n  border-radius: 50%;\n  background-color: deeppink;\n}\n.fade-enter-active, .fade-leave-active {\n  transition: opacity .5s;\n}\n.fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\n  opacity: 0;\n}\n\n.slide-enter,\n.slide-leave-to {\n  opacity: 0;\n}\n.slide-enter-active {\n  animation: slide-in 0.5s;\n  transition: opacity 5s;\n}\n.slide-leave-active {\n  animation: slide-in 0.5s reverse;\n  transition: opacity 5s;\n}\n\n@keyframes slide-in {\n  from {\n    transform: translateX(100px);\n  }\n  to {\n    transform: translateX(0);\n  }\n}\n\n.main {\n  width: 70%;\n  margin: auto;\n  padding-top: 5rem;\n  text-align: center;\n}\n</style>"]},"metadata":{},"sourceType":"module"}