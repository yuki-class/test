{"ast":null,"code":"import EventTitle from './EventTitle.vue';\nexport default {\n  data() {\n    return {\n      locations: [\"東京\", \"京都\", \"大阪\"],\n      eventData: {\n        title: 'タイトル',\n        maxNumber: 10,\n        host: 'ユキ',\n        detail: \"\",\n        isPrivate: false,\n        target: [],\n        price: \"有料\",\n        location: \"\"\n      }\n    };\n  },\n\n  components: {\n    EventTitle\n  }\n};","map":{"version":3,"mappings":"AA0CA,OAAOA,UAAP,MAAuB,kBAAvB;AACA,eAAe;EACXC,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,CAAC,IAAD,EAAO,IAAP,EAAa,IAAb,CADR;MAEHC,SAAS,EAAE;QACPC,KAAK,EAAE,MADA;QAEPC,SAAS,EAAE,EAFJ;QAGPC,IAAI,EAAE,IAHC;QAIPC,MAAM,EAAE,EAJD;QAKPC,SAAS,EAAE,KALJ;QAMPC,MAAM,EAAE,EAND;QAOPC,KAAK,EAAE,IAPA;QAQPC,QAAQ,EAAE;MARH;IAFR,CAAP;EAaH,CAfU;;EAgBXC,UAAU,EAAE;IACRZ;EADQ;AAhBD,CAAf","names":["EventTitle","data","locations","eventData","title","maxNumber","host","detail","isPrivate","target","price","location","components"],"sourceRoot":"","sources":["/Users/sasakiyuta/github/test/portfolio/src/components/FormVue.vue"],"sourcesContent":["<template>\n    <div>\n        <h2>フォーム</h2>\n        <EventTitle v-model=\"eventData.title\"></EventTitle>\n        \n        <label for=\"maxNumber\">最大人数</label>\n        <input id=\"maxNumber\" type=\"number\" v-model.number=\"eventData.maxNumber\">\n        <p>{{ eventData.maxNumber }}</p>\n\n        <label for=\"host\">主催者</label>\n        <input id=\"host\" type=\"text\" v-model.trim=\"eventData.host\">\n        <pre>{{ eventData.host }}</pre>\n\n        <label for=\"detail\">イベントの内容</label>\n        <textarea id=\"detail\" cols=\"30\" rows=\"10\" v-model=\"eventData.detail\">\n        </textarea>\n        <p style=\"white-space: pre;\">{{ eventData.detail}}</p>\n        <input type=\"checkbox\" id=\"isPrivate\" v-model=\"eventData.isPrivate\">\n        <label for=\"isPrivate\">非公開</label>\n        <p> {{ eventData.isPrivate }}</p>\n        <p>参加条件</p>\n        <input type=\"checkbox\" id=\"10\" value=\"10代\" v-model=\"eventData.target\">\n        <label for=\"10\">10代</label>\n        <input type=\"checkbox\" id=\"20\" value=\"20代\" v-model=\"eventData.target\">\n        <label for=\"20\">20代</label>\n        <input type=\"checkbox\" id=\"30\" value=\"30代\" v-model=\"eventData.target\">\n        <label for=\"30\">30代</label>\n        <p>{{ eventData.target }}</p>\n        <p>参加費</p>\n        <input type=\"radio\" id=\"free\" value=\"無料\" v-model=\"eventData.price\">\n        <label for=\"free\">無料</label>\n        <input type=\"radio\" id=\"paid\" value=\"有料\" v-model=\"eventData.price\">\n        <label for=\"paid\">有料</label>\n        <p>開催場所</p>\n        <select v-model=\"eventData.location\">\n        <option v-for=\"location in locations\" :key=\"location\">{{ location }}</option>\n        </select>\n        <p>{{ eventData.location }}</p>\n    </div>\n</template>\n\n<script>\nimport EventTitle from './EventTitle.vue'\nexport default {\n    data() {\n        return {\n            locations: [\"東京\", \"京都\", \"大阪\"],\n            eventData: {\n                title: 'タイトル',\n                maxNumber: 10,\n                host: 'ユキ',\n                detail: \"\",\n                isPrivate: false,\n                target: [],\n                price: \"有料\",\n                location: \"\"\n            }\n        }\n    },\n    components: {\n        EventTitle\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}